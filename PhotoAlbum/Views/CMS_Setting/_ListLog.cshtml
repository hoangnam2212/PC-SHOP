
@model List<CMSWebsite.Models.CMS_Log>
<table id="dtLstLog" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Người thao tác</th>
            <th>Nội dung</th>
            <th>Chi tiết</th>
            <th>Ngày giờ</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Fullname</td>
                <td>@item.Title</td>
                <td>@item.ContentLog</td>
                <td>@item.Date.ToString("dd/MM/yyyy lúc HH:mm:ss")</td>
            </tr>
        }
    </tbody>
</table>

<script>
    $(document).ready(function () {
        $('#dtLstLog').DataTable(normalOptionDataTable);
    });
</script>